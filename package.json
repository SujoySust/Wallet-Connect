    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "prisma format && prisma db push",
    "seed": "prisma db seed",
    "pm2:delete": "pm2 delete nft-backend-app || : && echo $HOME",
    "pm2:start": "pm2 start -n nft-backend --env production",
    "docker:dev": "prisma format && prisma db push && prisma db seed && nest start --watch",
    "docker:prod": "prisma format && prisma db push && prisma db seed && pm2 delete nft-backend-app || : && pm2 start -n nft-backend --env production"
